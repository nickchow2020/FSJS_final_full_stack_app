                Cookies.set(\"authenticatedUser\",JSON.stringify(user),{expires: 1})\n            }\n        })\n        return user\n    }\n\n    signOut = ()=>{\n        this.setState({\n            authenticatedUser: null\n        })\n\n        Cookies.remove(\"authenticatedUser\");\n        Cookies.remove(\"userpass\");\n    }\n\n    createCourse = (course)=>{\n        \n        const newCourse = {\n            ...course,\n            userId : 3\n        }   \n\n        const {\n            emailAddress:username\n        } = this.state.authenticatedUser\n\n        const password = atob(Cookies.get(\"userpass\"))\n\n        const addCourses = this.data.createCourse(newCourse,username,password)\n        return addCourses\n    }\n\n        updateCourse = async (id,data)=>{\n        const {\n            emailAddress:username\n        } = this.state.authenticatedUser\n\n        const password = atob(Cookies.get(\"userpass\"))\n\n        return await this.data.updateCourse(id,data,username,password)\n        .then(data =>{\n            return data\n        })\n    }\n\n    deleteCourse = (id)=>{\n        const {\n            emailAddress:username\n        } = this.state.authenticatedUser\n\n        const password = atob(Cookies.get(\"userpass\"))\n\n        this.data.deleteCourse(id,username,password)\n    }\n\n    render(){\n\n        const value = {\n            data: this.data,\n            authenticatedUser: this.state.authenticatedUser,\n            actions: {\n                signIn : this.signIn,\n                signOut : this.signOut,\n                createCourse: this.createCourse,\n                updateCourse: this.updateCourse,\n                deleteCourse: this.deleteCourse,\n            }\n        }\n\n        return(\n            <Context.Provider value={value}>\n                {this.props.children}\n            </Context.Provider>\n        )\n    }\n}\n\nexport const Consumer = Context.Consumer;\n\n\nexport default  (Component)=>{\n    return  (props)=>{\n        return(\n            <Context.Consumer>\n                {context => <Component {...props} context={context} />}\n            </Context.Consumer>\n        )\n    }\n}",["116","117"],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/components/UpdateCourse.js",[],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/components/Courses.js",[],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/components/CourseDetail.js",[],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/components/CourseDetailButtons.js",[],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/Data.js",[],"/Users/stephenzhou/Documents/Full_Stack_TD/FSJS_final_full_stack_app/client/src/AuthorizatedRoute.js",[],{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","replacedBy":"121"},{"ruleId":"122","severity":1,"message":"123","line":17,"column":8,"nodeType":"124","messageId":"125","endLine":17,"endColumn":21},{"ruleId":"126","severity":1,"message":"127","line":108,"column":1,"nodeType":"128","endLine":116,"endColumn":2},{"ruleId":"118","replacedBy":"129"},{"ruleId":"120","replacedBy":"130"},{"ruleId":"126","severity":1,"message":"127","line":3,"column":1,"nodeType":"128","endLine":9,"endColumn":2},{"ruleId":"118","replacedBy":"131"},{"ruleId":"120","replacedBy":"132"},{"ruleId":"122","severity":1,"message":"133","line":3,"column":8,"nodeType":"124","messageId":"125","endLine":3,"endColumn":14},{"ruleId":"126","severity":1,"message":"127","line":109,"column":1,"nodeType":"128","endLine":117,"endColumn":2},{"ruleId":"118","replacedBy":"134"},{"ruleId":"120","replacedBy":"135"},"no-native-reassign",["136"],"no-negated-in-lhs",["137"],"no-unused-vars","'Authenticated' is defined but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign arrow function to a variable before exporting as module default","ExportDefaultDecl